  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress --config config/webpack.config.dev.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "precommit": "lint-staged",
    "build:prod": "NODE_ENV=production webpack --bail --progress --config config/webpack.config.prod.js",
    "prettier": "prettier --write --config ./.prettierrc src/**/*.{js,css}"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\]",
      "^.+\\.module\\.css$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.css$": "identity-obj-proxy",
      "\\.(css)$": "<rootDir>/config/jest/cssTransform.js",
      "^actions/(.*)$": "<rootDir>/src/actions/$1",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^containers/(.*)$": "<rootDir>/src/containers/$1",
      "^consumers/(.*)$": "<rootDir>/src/consumers/$1",
      "^constants/(.*)$": "<rootDir>/src/constants/$1",
      "^@graphql/(.*)$": "<rootDir>/src/graphql/$1",
      "^reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^sagas/(.*)$": "<rootDir>/src/sagas/$1",
      "^selectors/(.*)$": "<rootDir>/src/selectors/$1",
      "react-i18next": "<rootDir>/config/jest/reacti18nextMock.js"
    },
    "modulePathIgnorePatterns": [
      "__mocks__"
    ],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "ts",
      "tsx"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
